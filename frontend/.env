# frontend/.env.example
# Copier ce fichier en .env pour votre configuration locale

# URL de l'API Backend
REACT_APP_API_URL=http://localhost:8000

# Environnement
REACT_APP_ENV=development

# Intervalle de rafraîchissement des flux RSS (en millisecondes)
REACT_APP_REFRESH_INTERVAL=300000

# Activer les logs de debug
REACT_APP_DEBUG=true

ENVIRONMENT=development

# Configuration PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=mypassword
POSTGRES_DB=suprss_bd

# Clé secrète pour l'application (générez une clé forte de 32+ caractères)
SECRET_KEY=changez_cette_cle_secrete_en_production

# Configuration PgAdmin
PGADMIN_EMAIL=admin@suprss.com
PGADMIN_PASSWORD=admin
